{
    "name": "soap/laravel-websockets-chat-demo",
    "type": "project",
    "description": "Prototype chat application using Laravel Jetstream, InertiaJS and self hosted Websockets",
    "keywords": ["vue 3", "laravel", "jetstream", "inertia", "laravel-websockets", "pusher aternative"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "beyondcode/laravel-websockets": "^1.12",
        "guzzlehttp/guzzle": "7.2",
        "inertiajs/inertia-laravel": "^0.6",
        "laravel/framework": "^10.0",
        "laravel/jetstream": "^2.3",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.5",
        "pusher/pusher-php-server": "~5.0",
        "tightenco/ziggy": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "spatie/laravel-ignition": "^2.0",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
